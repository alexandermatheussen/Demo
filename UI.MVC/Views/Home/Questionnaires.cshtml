@model IEnumerable<Domain.Questionnaire>

<h1>Questionnaires</h1>

<table class="table">
    <tr>
        <th>
            <p>name</p>
        </th>
        <th>
            <p>Amount of questions</p>
        </th>
    </tr>
    @foreach (var item in Model)
    {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.questionAmount)
                </td>
                <td>
                    @Html.ActionLink("Complete Questionnaire", "Questionnaire", "Home"
                        , new { id=item.id })
                </td>
            </tr>
        
    }
</table>
